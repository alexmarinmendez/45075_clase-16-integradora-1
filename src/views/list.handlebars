<div>
    {{#each pokemons}}
    <div>
        <a href="/pokedex/{{this.name}}">
            <b>{{this.name}}</b>
            <img src="{{this.photo}}" height="30">
            <i>{{this.type}}</i>
        </a>
    </div>
    {{/each}}
    <button type="button" class="btn btn-primary">Primary</button>
</div>